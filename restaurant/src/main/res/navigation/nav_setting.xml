<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_setting"
    app:startDestination="@id/settingFragment">

    <fragment
        android:id="@+id/settingFragment"
        android:name="com.dsm.restaurant.presentation.ui.main.setting.SettingFragment"
        android:label="SettingFragment"
        tools:layout="@layout/fragment_setting" >
        <action
            android:id="@+id/action_settingFragment_to_changePwdDialog"
            app:destination="@id/changePwdDialog" />
    </fragment>
    <dialog
        android:id="@+id/changePwdDialog"
        android:name="com.dsm.restaurant.presentation.ui.main.setting.ChangePwdDialog"
        android:label="ChangePwdDialog"
        tools:layout="@layout/dialog_change_pwd"/>
</navigation>